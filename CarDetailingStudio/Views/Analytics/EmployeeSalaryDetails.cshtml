
<div class="row">
    <div class="col-md-3">
        <div class="card">
            <div class="card-body">
                <p>AdministratorCarWash</p>
                <table class="table table-hover">
                    @foreach (var item in ViewBag.AdministratorCarWash)
                    {
                        <tr>
                            <td>@item.Payroll</td>
                            <td>@item.IdCarWashWorkers</td>
                            <td>@item.IdOrder</td>
                        </tr>
                    }
                </table>
                <hr />
                <h4>Сумма ЗП админа мойки - <b class="text-center">@ViewBag.AdministratorSum</b></h4>
                <hr />
            </div>
        </div>
    </div>
