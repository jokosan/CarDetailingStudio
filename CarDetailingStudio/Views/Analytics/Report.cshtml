                                    </tr>
                                    <tr>
                                        <td class="text-center">1</td>
                                        <td>Начало дня</td>
                                        <td></td>
                                        <td class="text-right"> @Html.DisplayFor(model => model.cashStartOfTheDay) грн. </td>
                                    </tr>
                                    <tr>
                                        <td colspan="6" class="text-center"><b class="text-danger">Итого</b></td>
                                    </tr>
